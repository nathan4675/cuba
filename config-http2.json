{
  "log": {
    "loglevel": "debug"
  },
  "inbounds": [
    {
      "port": "YOUR_PORT",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "YOUR_UUID"
          }
        ]
      },
      "streamSettings": {
        "network": "http",
        "security": "none",
        "httpSettings": {
          "path": "/YOUR_PATH"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}